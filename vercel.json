{
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/civiltakeoff",
      "destination": "/civiltakeoff.html"
    },
    {
      "source": "/propdfeditor",
      "destination": "/propdfeditor.html"
    },
    {
      "source": "/bmicalculator",
      "destination": "/bmicalculator.html"
    },
    {
      "source": "/texteditor",
      "destination": "/texteditor.html"
    },
    {
      "source": "/fitlifeplanner",
      "destination": "/fitlifeplanner.html"
    },
    {
      "source": "/photoeditorpro",
      "destination": "/photoeditorpro.html"
    },
    {
      "source": "/universalunitconverter",
      "destination": "/universalunitconverter.html"
    },
    {
      "source": "/codekithub",
      "destination": "/codekithub.html"
    },
    {
      "source": "/agecalculator",
      "destination": "/agecalculator.html"
    },
    {
      "source": "/countdown-stopwatch",
      "destination": "/countdown-stopwatch.html"
    },
    {
      "source": "/dayoftheweek",
      "destination": "/dayoftheweek.html"
    },
    {
      "source": "/scientificcalculator",
      "destination": "/scientificcalculator.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        }
      ]
    }
  ]
}
